{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "ENABLE_TOOL_SEARCH": "true"
  },
  "model": "opusplan",
  "statusLine": {
    "type": "command",
    "command": "bunx ccusage statusline",
    "padding": 0
  },
  "enabledPlugins": {
    "frontend-design@claude-code-plugins": true,
    "example-skills@anthropic-agent-skills": true,
